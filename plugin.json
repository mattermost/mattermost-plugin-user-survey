{
    "id": "com.mattermost.user-survey",
    "name": "User Survey",
    "description": "A plugin designed for gathering user feedback on Mattermost and related topics.",
    "homepage_url": "https://github.com/mattermost/mattermost-plugin-user-survey",
    "support_url": "https://github.com/mattermost/mattermost-plugin-user-survey/issues",
    "icon_path": "assets/starter-template-icon.svg",
    "min_server_version": "6.2.1",
    "server": {
        "executables": {
            "linux-amd64": "server/dist/plugin-linux-amd64",
            "linux-arm64": "server/dist/plugin-linux-arm64",
            "darwin-amd64": "server/dist/plugin-darwin-amd64",
            "darwin-arm64": "server/dist/plugin-darwin-arm64",
            "windows-amd64": "server/dist/plugin-windows-amd64.exe"
        }
    },
    "webapp": {
        "bundle_path": "webapp/dist/main.js"
    },
    "settings_schema": {
        "settings": [
            {
                "key": "SurveyDateTime",
                "display_name": "Send next survey at:",
                "type": "custom"
            },
            {
                "key": "SurveyExpiry",
                "display_name": "Survey expiry (days):",
                "type": "custom"
            },
            {
                "key": "TeamFilter",
                "display_name": "Exclude specific teams:",
                "type": "custom"
            },
            {
                "key": "SurveyQuestions",
                "display_name": "Survey Questions:",
                "type": "custom"
            }
        ]
    }
}
