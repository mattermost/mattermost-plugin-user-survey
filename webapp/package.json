{
  "private": true,
  "scripts": {
    "build": "webpack --mode=production",
    "build:watch": "webpack --mode=production --watch",
    "debug": "webpack --mode=none",
    "debug:watch": "webpack --mode=development --watch",
    "lint": "eslint --ignore-pattern node_modules --ignore-pattern dist --ext .js --ext .jsx --ext tsx --ext ts ./src --quiet --cache",
    "fix": "eslint --ignore-pattern node_modules --ignore-pattern dist --ext .js --ext .jsx --ext tsx --ext ts ./src --quiet --fix --cache",
    "test": "jest --forceExit --detectOpenHandles --verbose",
    "test:watch": "jest --watch",
    "test-ci": "jest --forceExit --detectOpenHandles --maxWorkers=2",
    "check-types": "tsc"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "6.4.2",
    "@types/jest": "29.5.12",
    "@types/node": "20.11.20",
    "@types/react": "18.2.58",
    "@types/react-dom": "18.2.19",
    "@types/react-redux": "7.1.33",
    "@types/react-router-dom": "5.3.3",
    "@types/react-transition-group": "4.4.10",
    "@typescript-eslint/eslint-plugin": "7.0.2",
    "@typescript-eslint/parser": "7.0.2",
    "@typescript-eslint/typescript-estree": "7.0.2",
    "css-loader": "6.10.0",
    "eslint": "8.57.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-import-resolver-webpack": "0.13.8",
    "eslint-plugin-cypress": "2.15.1",
    "eslint-plugin-header": "3.1.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "27.9.0",
    "eslint-plugin-mattermost": "github:mattermost/eslint-plugin-mattermost",
    "eslint-plugin-no-only-tests": "3.1.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "file-loader": "6.2.0",
    "identity-obj-proxy": "3.0.0",
    "isomorphic-fetch": "3.0.0",
    "jest": "29.7.0",
    "jest-canvas-mock": "2.5.2",
    "jest-junit": "16.0.0",
    "sass": "1.71.1",
    "sass-loader": "14.1.1",
    "style-loader": "3.3.4",
    "ts-loader": "9.5.1",
    "webpack": "5.90.3",
    "webpack-cli": "5.1.4"
  },
  "dependencies": {
    "@floating-ui/react": "0.26.9",
    "@mattermost/compass-icons": "0.1.43",
    "classnames": "2.5.1",
    "core-js": "3.36.0",
    "date-fns": "3.3.1",
    "mattermost-redux": "5.33.1",
    "react": "18.2.0",
    "react-day-picker": "8.10.0",
    "react-redux": "8.0.5",
    "react-select": "5.8.0",
    "redux": "4.2.1",
    "typescript": "4.9.5"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/non_npm_dependencies/"
    ],
    "clearMocks": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coverageReporters": [
      "lcov",
      "text-summary"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "identity-obj-proxy",
      "^.+\\.(css|less|scss)$": "identity-obj-proxy",
      "^.*i18n.*\\.(json)$": "<rootDir>/tests/i18n_mock.json",
      "^bundle-loader\\?lazy\\!(.*)$": "$1"
    },
    "moduleDirectories": [
      "",
      "node_modules",
      "non_npm_dependencies"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|react-router|mattermost-webapp)"
    ],
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.tsx"
    ],
    "testURL": "http://localhost:8065"
  }
}
